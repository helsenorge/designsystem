{
  "name": "@helsenorge/designsystem-elements",
  "version": "0.0.1-dev.1",
  "description": "The official design system for Helsenorge built with StencilJS.",
  "repository": {
    "type": "git",
    "url": "https://github.com/helsenorge/designsystem",
    "directory": "packages/designsystem-elements"
  },
  "homepage": "https://helsenorge.design",
  "author": "Steffen Lien <steffen.lien@ehelse.no>",
  "bugs": {
    "url": "https://github.com/Helsenorge/designsystem/issues",
    "email": "contact@helsenorge.design"
  },
  "license": "MIT",
  "keywords": [
    "components",
    "design-system",
    "helsenorge",
    "patterns",
    "stenciljs",
    "web-components"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/designsystem-elements/designsystem-elements.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "clean": "rimraf dist www loader",
    "build": "stencil build",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|ts|tsx)\"",
    "precommit": "lint-staged",
    "generate": "stencil generate",
    "lint": "eslint \"src/**/*\"",
    "test": "stencil test --spec",
    "test:watch": "stencil test --spec --watchAll",
    "start": "stencil build --dev --watch --serve",
    "validate": "yarn lint && yarn format -l"
  },
  "devDependencies": {
    "@stencil/core": "^1.7.5",
    "@stencil/eslint-plugin": "^0.2.1"
  }
}
