{
  "name": "@helsenorge/designsystem-react",
  "sideEffects": false,
  "version": "1.0.0-beta36",
  "description": "The official design system for Helsenorge built with React.",
  "repository": {
    "type": "git",
    "url": "https://github.com/helsenorge/designsystem",
    "directory": "packages/designsystem-react"
  },
  "homepage": "https://helsenorge.design",
  "author": "Helsenorge ROX",
  "bugs": {
    "url": "https://github.com/Helsenorge/designsystem/issues",
    "email": "contact@helsenorge.design"
  },
  "license": "MIT",
  "keywords": [
    "components",
    "design-system",
    "helsenorge",
    "patterns",
    "react"
  ],
  "publishConfig": {
    "access": "public"
  },
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "install:packages": "npm ci",
    "prebuild": "npm run generate:entries && npm run clean",
    "build": "rollup -c --environment BUILD:production && npm run generate:tsdefinitions && npm run copy:npmcontent",
    "publish:package": "npm publish ./dist --tag=beta",
    "clean": "rimraf dist types",
    "cz": "git-cz",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|ts|tsx)\"",
    "generate:entries": "node scripts/entriesGenerator.js && prettier --write \"scripts/componentsEntries.json\"",
    "generate:tsdefinitions": "tsc --emitDeclarationOnly --project tsconfig.json && copyfiles --up 1 \"./types/**/*.ts\" \"./dist\" -E",
    "generate:icons": "node scripts/iconGenerator.js generate",
    "copy:npmcontent": "node scripts/packageJsonGenerator.js && copyfiles \"*.md\" \"./dist\" -E",
    "lint": "eslint \"src/**/*\"",
    "precommit": "lint-staged",
    "test": "jest",
    "test:open": "open -a 'Google Chrome' coverage/lcov-report/index.html",
    "test:watch": "jest --watch",
    "start": "start-storybook",
    "validate": "npm run lint && npm run format -l"
  },
  "peerDependencies": {
    "bootstrap": "4.4.1",
    "normalize.css": "8.0.1",
    "react": ">= 16.12.0",
    "react-dom": ">= 16.12.0",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@rollup/plugin-typescript": "^4.1.2",
    "@storybook/addon-a11y": "^5.3.13",
    "@storybook/addon-actions": "^5.3.12",
    "@storybook/addon-backgrounds": "^5.3.12",
    "@storybook/addon-centered": "^5.3.12",
    "@storybook/addon-knobs": "^5.3.12",
    "@storybook/addon-viewport": "^5.3.12",
    "@storybook/addons": "^5.3.12",
    "@storybook/react": "^5.3.12",
    "@svgr/core": "^4.3.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^24.9.0",
    "@types/styled-components": "^5.0.1",
    "@types/testing-library__react": "^9.1.2",
    "async": "^3.1.0",
    "babel-loader": "^8.0.6",
    "copyfiles": "^2.2.0",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-styled-components": "^7.0.0",
    "jest-watch-typeahead": "^0.4.2",
    "normalize.css": "8.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "react": "16.12.0",
    "react-docgen-typescript-loader": "^3.3.0",
    "react-dom": "16.12.0",
    "resolve-url-loader": "^3.1.1",
    "rollup": "^2.10.9",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-execute": "^1.1.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-scss": "^2.5.0",
    "rollup-plugin-terser": "^6.1.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "styled-components": "5.0.1",
    "svg-parser": "^2.0.2",
    "svgo": "^1.3.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.2",
    "yargs": "^15.0.2"
  },
  "dependencies": {
    "bootstrap": "4.4.1"
  }
}
