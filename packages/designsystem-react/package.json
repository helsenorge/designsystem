    "jest-watch-typeahead": "^0.4.2",
    "normalize.css": "8.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "react": "16.12.0",
    "react-docgen-typescript-loader": "^3.3.0",
    "react-dom": "16.12.0",
    "resolve-url-loader": "^3.1.1",
