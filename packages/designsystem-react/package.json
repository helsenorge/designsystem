{
  "name": "@helsenorge/designsystem-react",
  "version": "1.0.0-beta.1",
  "description": "The official design system for Helsenorge built with React.",
  "repository": {
    "type": "git",
    "url": "https://github.com/helsenorge/designsystem",
    "directory": "packages/designsystem-react"
  },
  "homepage": "https://helsenorge.design",
  "author": "Steffen Lien <steffen.lien@ehelse.no>",
  "bugs": {
    "url": "https://github.com/Helsenorge/designsystem/issues",
    "email": "contact@helsenorge.design"
  },
  "license": "MIT",
  "keywords": [
    "components",
    "design-system",
    "helsenorge",
    "patterns",
    "react"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/js/helsenorge.js",
  "module": "dist/js/helsenorge.esm.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "yarn clean && rollup -c --environment BUILD:production && yarn build:defs",
    "build:defs": "tsc --emitDeclarationOnly && dts-bundle-generator -project ./tsconfig.json -o ./dist/index.d.ts ./types/src/index.d.ts",
    "clean": "rimraf dist types",
    "cz": "git-cz",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|ts|tsx)\"",
    "generate:icons": "node scripts/iconGenerator.js generate",
    "lint": "eslint \"src/**/*\"",
    "precommit": "lint-staged",
    "test": "jest",
    "test:open": "open -a 'Google Chrome' coverage/lcov-report/index.html",
    "test:watch": "jest --watch",
    "start": "start-storybook",
    "validate": "yarn lint && yarn format -l"
  },
  "peerDependencies": {
    "bootstrap": "4.4.1",
    "normalize.css": "8.0.1",
    "react": ">= 16.12.0",
    "react-dom": ">= 16.12.0",
    "styled-components": "^5.0.0"
  },
