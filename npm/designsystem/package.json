{
  "name": "@helsenorge/designsystem-react",
  "sideEffects": false,
  "version": "1.0.0-beta129",
  "description": "The official design system for Helsenorge built with React.",
  "repository": {
    "type": "tfs",
    "url": "https://dev.azure.com/nhnfelles/Helsenorge/_git/HN-Designsystem"
  },
  "homepage": "https://helsenorge.design",
  "author": "Helsenorge",
  "maintainers": [
    "ROX"
  ],
  "license": "MIT",
  "publishConfig": {
    "directory": "lib",
    "access": "public"
  },
  "types": "lib/index.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "start": "start-storybook",
    "prebuild": "npm-run-all generate:entries clean",
    "build": "rollup -c --environment BUILD:production",
    "build-storybook": "build-storybook -o dist",
    "postbuild": "npm-run-all generate:cssdefinitions generate:tsdefinitions copy:npmcontent copy:scss",
    "publish:package": "npm publish ./lib --tag=beta",
    "clean": "rimraf lib lib_npm types",
    "format": "prettier --write \"src/**/*.+(js|jsx|json|ts|tsx)\"",
    "format:ci": "prettier --check \"src/**/*.+(js|jsx|json|ts|tsx)\"",
    "generate:entries": "node scripts/entriesGenerator.js && prettier --write \"scripts/componentsEntries.json\"",
    "generate:cssdefinitions": "tsm src --nameFormat none --exportType default",
    "generate:tsdefinitions": "tsc --emitDeclarationOnly --project tsconfig.json && copyfiles --up 1 \"./types/**/*.ts\" \"./lib\" -E",
    "generate:icons": "node scripts/iconGenerator.js generate",
    "copy:npmcontent": "node scripts/packageJsonGenerator.js && copyfiles \".npmrc\" \"./lib\" -E && copyfiles \"*.md\" \"./lib\" -E",
    "copy:libnpmjs": "node scripts/createNpmrc.js && copyfiles -u 1 \"./lib/**/*\" \"./lib_npm\" -E",
    "copy:scss": "copyfiles -V -u 1 src/components/**/*.{scss*,json,js} lib",
    "lint": "eslint \"src/**/*\"",
    "precommit": "lint-staged",
    "test": "jest",
    "test:update": "jest -u",
    "test:open": "open -a 'Google Chrome' coverage/lcov-report/index.html",
    "test:watch": "jest --watch",
    "validate": "npm run lint && npm run format -l"
  },
  "peerDependencies": {
    "bootstrap": "4.6.1",
    "normalize.css": "8.0.1",
    "react": "^16.0.0 || ^17.0.0",
    "react-dom": "^16.0.0 || ^17.0.0"
  },
  "dependencies": {
    "bootstrap": "4.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@rollup/plugin-typescript": "^4.1.2",
    "@storybook/addon-a11y": "^5.3.21",
    "@storybook/addon-actions": "^5.3.21",
    "@storybook/addon-backgrounds": "^5.3.21",
    "@storybook/addon-centered": "^5.3.21",
    "@storybook/addon-knobs": "^5.3.21",
    "@storybook/addon-viewport": "^5.3.21",
    "@storybook/addons": "^5.3.21",
    "@storybook/react": "^5.3.21",
    "@svgr/core": "^4.3.3",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.0.5",
    "@testing-library/user-event": "^13.1.8",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^25.2.3",
    "@types/prettier": "1.19.0",
    "@types/react": "^16.0.0",
    "@types/react-dom": "^16.0.0",
    "@types/testing-library__react": "^10.0.5",
    "async": "^3.1.0",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "copyfiles": "^2.2.0",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "jest-watch-typeahead": "^0.4.2",
    "jest-when": "^3.5.1",
    "node-sass": "^4.13.0",
    "normalize.css": "8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "1.19.1",
    "react": "^16.0.0",
    "react-docgen-typescript": "^1.22.0",
    "react-docgen-typescript-loader": "^3.3.0",
    "react-dom": "^16.0.0",
    "react-hook-form": "^6.15.8",
    "resolve-url-loader": "^3.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.9",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-execute": "^1.1.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "svg-parser": "^2.0.2",
    "svgo": "^1.3.2",
    "ts-loader": "^6.2.1",
    "typed-scss-modules": "^4.1.1",
    "typescript": "~3.8.3",
    "webpack": "^4.41.2",
    "yargs": "^15.0.2"
  }
}
