{
  "name": "@helsenorge/designsystem-react",
  "sideEffects": false,
  "version": "2.2.1",
  "description": "The official design system for Helsenorge built with React.",
  "repository": {
    "type": "tfs",
    "url": "https://dev.azure.com/nhnfelles/Helsenorge/_git/HN-Designsystem"
  },
  "homepage": "https://helsenorge.design",
  "author": "Helsenorge",
  "maintainers": [
    "ROX"
  ],
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "directory": "lib",
    "access": "public"
  },
  "scripts": {
    "start": "start-storybook",
    "prebuild": "npm-run-all clean generate:cssdefinitions generate:tsdefinitions",
    "build": "vite build",
    "build-storybook": "build-storybook -o dist",
    "clean": "rimraf lib types",
    "generate:cssdefinitions": "typed-scss-modules \"src/**/*.module.scss\"",
    "generate:tsdefinitions": "tsc --emitDeclarationOnly",
    "generate:componentdata": "node scripts/componentdata.js",
    "create-npmrc": "node scripts/createNpmrc.js",
    "pregenerate:figmatokens": "git -C ../../../designtokens pull https://github.com/helsenorge/designtokens.git",
    "generate:figmatokens": "token-transformer ../../../designtokens/tokens.json data/design-tokens.json hn-designsystem --resolveReferences=false",
    "postgenerate:figmatokens": "style-dictionary build --config figma-token.config.json",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check src/**/*.{js,jsx,ts,tsx,css,scss,md,json}",
    "prettier:fix": "npm run prettier -- --write",
    "pretypecheck": "tsc -v",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest --u"
  },
  "peerDependencies": {
    "bootstrap": "^4.6.2",
    "classnames": "^2.3.2",
    "normalize.css": "^8.0.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-replace": "^4.0.0",
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-backgrounds": "^6.5.12",
    "@storybook/addon-controls": "^6.5.12",
    "@storybook/addon-docs": "^6.5.12",
    "@storybook/addon-viewport": "^6.5.12",
    "@storybook/addons": "^6.5.12",
    "@storybook/builder-vite": "^0.2.3",
    "@storybook/react": "^6.5.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.1.1",
    "@types/react": "^17.0.50",
    "@types/react-dom": "17.0.17",
    "bootstrap": "^4.6.2",
    "classnames": "^2.3.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "jest-when": "^3.5.1",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^6.15.8",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "sass": "^1.55.0",
    "style-dictionary": "^3.7.1",
    "token-transformer": "^0.0.27",
    "typed-scss-modules": "^7.0.1",
    "typescript": "~4.8.4",
    "vite": "^3.1.4"
  }
}
