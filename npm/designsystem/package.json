{
  "name": "@helsenorge/designsystem-react",
  "sideEffects": false,
  "version": "4.4.0",
  "description": "The official design system for Helsenorge built with React.",
  "repository": {
    "type": "git",
    "url": "https://github.com/helsenorge/designsystem"
  },
  "homepage": "https://helsenorge.design",
  "author": "Helsenorge",
  "maintainers": [
    "ROX"
  ],
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "directory": "lib",
    "access": "public"
  },
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook",
    "prebuild": "npm-run-all clean generate:cssdefinitions generate:tsdefinitions",
    "build": "vite build",
    "build-storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider build-storybook -o dist",
    "clean": "rimraf lib types",
    "generate:cssdefinitions": "typed-scss-modules \"src/**/*.module.scss\"",
    "generate:tsdefinitions": "tsc --emitDeclarationOnly",
    "generate:componentdata": "node scripts/componentdata.js",
    "pregenerate:figmatokens": "git -C ../../../designtokens pull https://github.com/helsenorge/designtokens.git",
    "generate:figmatokens": "token-transformer ../../../designtokens/tokens.json data/design-tokens.json hn-designsystem --resolveReferences=false",
    "postgenerate:figmatokens": "style-dictionary build --config figma-token.config.json",
    "deploy:storybook": "netlify deploy --site 69e5c387-fee4-47d2-9498-67068981bee7 --dir dist --prod",
    "deploy:storybook:next": "netlify deploy --site 69e5c387-fee4-47d2-9498-67068981bee7 --dir dist --alias next",
    "eslint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "prettier": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,scss,md,json}\"",
    "prettier:fix": "npm run prettier -- --write",
    "pretypecheck": "tsc -v",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:update": "jest --u",
    "new": "node scripts/new-component.js"
  },
  "peerDependencies": {
    "bootstrap": "^4.6.2",
    "classnames": "^2.3.2",
    "normalize.css": "^8.0.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-backgrounds": "^6.5.16",
    "@storybook/addon-controls": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-viewport": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/react": "^6.5.16",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/react": "^17.0.53",
    "@types/react-dom": "17.0.19",
    "@vitejs/plugin-react": "^3.1.0",
    "bootstrap": "^4.6.2",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "glob": "^7.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-when": "^3.5.2",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.43.7",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "sass": "^1.59.3",
    "style-dictionary": "^3.7.2",
    "token-transformer": "^0.0.30",
    "typed-scss-modules": "^7.1.0",
    "typescript": "~4.9.5",
    "vite": "^4.2.1"
  },
  "dependencies": {
    "storybook-addon-html-validator": "^0.0.12"
  }
}
