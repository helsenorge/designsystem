{
  "name": "@helsenorge/designsystem",
  "version": "0.0.1-dev.12",
  "main": "./dist/js/helsenorge.js",
  "repository": "https://github.com/Helsenorge/designsystem.git",
  "homepage": "https://design.helsenorge.io",
  "description": "The official design system for Helsenorge.",
  "author": "Steffen Lien <steffen.lien@ehelse.no>",
  "bugs": {
    "url": "https://github.com/Helsenorge/designsystem/issues",
    "email": "design@helsenorge.io"
  },
  "license": "MIT",
  "keywords": [
    "components",
    "designsystem",
    "helsenorge",
    "patterns",
    "react"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && NODE_ENV=production rollup -c --environment BUILD:production && yarn build:dts",
    "build:dts": "tsc --emitDeclarationOnly && dts-bundle-generator -project ./tsconfig.json -o ./dist/indexs.d.ts ./types/index.d.ts",
    "bundle-analyze": "webpack --profile --json > bundle-stats.json && webpack-bundle-analyzer ./bundle-stats.json",
    "clean": "rimraf dist types",
    "format": "npm run prettier -- --write",
    "lint": "eslint .",
    "prerelease": "npm version prerelease",
    "prettier": "prettier \"src/**/*.+(js|jsx|json|yml|yaml|scss|ts|tsx)\"",
    "publish:dev": "np --tag=dev --no-release-draft --any-branch --no-publish",
    "publish:dev2": "npm publish --tag dev --dry-run",
    "start": "start-storybook -p 6006 -s ./src/images",
    "test": "jest",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test:watch": "jest --watch",
    "icons:generate": "npx @svgr/cli --ref --no-view-box false --replace-attr-values '#000={color}' --svg-props width={size} --svg-props height={size} --template ./scripts/iconTemplate.js --ext tsx -d ./src/components/Icons/  ./src/icons",
    "icons:inject": "node ./scripts/iconInject.js",
    "validate": "npm run lint && npm run prettier -- --list-different"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "module": "./dist/js/helsenorge.esm.js",
  "source": "src/index.ts",
  "files": [
    "dist/**/*"
  ],
  "types": "./dist/index.d.ts",
  "typescript": {
    "declaration": "./dist/index.d.ts"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-backgrounds": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addon-viewport": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "5.1.11",
    "@svgr/cli": "^4.3.0",
    "@svgr/webpack": "^4.3.0",
    "@testing-library/react": "^8.0.1",
    "@types/classnames": "^2.2.7",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@types/react-inlinesvg": "^0.8.1",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__addon-knobs": "^5.0.3",
    "@types/storybook__react": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "dts-bundle": "^0.7.3",
    "dts-bundle-generator": "^3.1.0",
    "dts-gen": "^0.5.8",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^4.0.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "jest-axe": "^3.1.1",
    "jest-dom": "^3.1.3",
    "jest-watch-typeahead": "^0.2.1",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "np": "^5.0.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "progress-bar-webpack-plugin": "^1.12.1",
    "react": "^16.8.5",
    "react-docgen-typescript-loader": "^3.0.1",
    "react-dom": "^16.8.5",
    "react-inlinesvg": "^0.8.4",
    "react-svg-loader": "^3.0.3",
    "react-testing-library": "^6.0.2",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.20.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-dts": "^1.1.6",
    "rollup-plugin-execute": "^1.1.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-progress": "^1.1.1",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-scss": "^1.0.2",
    "rollup-plugin-sizes": "^0.5.1",
    "rollup-plugin-typescript2": "^0.23.0",
    "rollup-pluginutils": "^2.8.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "svg-react-loader": "^0.4.6",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.4000",
    "url-loader": "^2.0.0",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  }
}
