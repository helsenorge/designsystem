{
  "name": "@helsenorge/designsystem",
  "version": "0.0.1-dev.2",
  "main": "./lib/index.js",
  "repository": "https://github.com/Helsenorge/designsystem.git",
  "homepage": "https://design.helsenorge.io",
  "description": "The official design system for Helsenorge.",
  "author": "Steffen Lien <steffen.lien@ehelse.no>",
  "bugs": {
    "url": "https://github.com/Helsenorge/designsystem/issues",
    "email": "design@helsenorge.io"
  },
  "license": "MIT",
  "keywords": [
    "components",
    "designsystem",
    "helsenorge",
    "patterns",
    "react"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && webpack --mode=production && yarn bundle-ts",
    "bundle-ts": "yarn dts-bundle --name @helsenorge/designsystem --main types/index.d.ts --baseDir types/ --out ../lib/index.d.ts --outputAsModuleFolder true",
    "bundle-analyze": "webpack --profile --json > bundle-stats.json && webpack-bundle-analyzer ./bundle-stats.json",
    "clean": "rimraf lib types",
    "format": "npm run prettier -- --write",
    "lint": "eslint .",
    "prerelease": "npm version prerelease",
    "prettier": "prettier \"src/**/*.+(js|jsx|json|yml|yaml|scss|ts|tsx)\"",
    "publish:dev": "npm publish --tag dev --dry-run",
    "start": "start-storybook -p 6006",
    "test": "jest",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test:watch": "jest --watch",
    "validate": "npm run lint && npm run prettier -- --list-different"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "source": "src/index.ts",
  "files": [
    "lib/**/*"
  ],
  "typescript": {
    "declaration": "./lib/index.d.ts"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/addon-knobs": "^5.0.6",
    "@storybook/addon-links": "^5.0.5",
    "@storybook/addon-viewport": "^5.0.6",
    "@storybook/addons": "^5.0.5",
    "@storybook/react": "5.1.0-alpha.39",
    "@types/classnames": "^2.2.7",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.7",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-knobs": "^5.0.0",
    "@types/storybook__react": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "dts-bundle": "^0.7.3",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "jest-axe": "^3.1.1",
    "jest-dom": "^3.1.3",
    "jest-watch-typeahead": "^0.2.1",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "react": "^16.8.5",
    "react-docgen-typescript-loader": "^3.0.1",
    "react-dom": "^16.8.5",
    "react-testing-library": "^6.0.2",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.4000",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  }
}
